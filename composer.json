{
    "name": "atoms/log",
    "description": "An implementation of PSR-3.",
    "homepage": "https://bitbucket.org/gotamedia/log",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Pettersson",
            "email": "peter.pettersson@gotamedia.se"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "psr/log": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2",
        "squizlabs/php_codesniffer": "~3.0",
        "phpmd/phpmd": "~2.6",
        "phan/phan": "~1.1"
    },
    "scripts": {
        "test": "exit 0",
